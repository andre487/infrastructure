logoutput: syslog
internal: 0.0.0.0 port = 8080
external: eth0

socksmethod: pam.username
user.privileged: root
user.unprivileged: nobody

client block {
        from:  0/0 to: lo
        log: error
}

client pass {
        from: 0/0 to: 0/0
        log: error connect
}

socks pass {
        from: 0/0 to: 0/0
        log: error connect
}
