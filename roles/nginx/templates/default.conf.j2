server {
    listen 80 default_server;
    listen [::]:80 default_server;

    root /var/www/common;

    index index.html index.htm;

    server_name _;

    location / {
        try_files $uri $uri/ =404;
    }
}

server {
    listen 127.0.0.1:80;

    server_name 127.0.0.1;

    location /nginx_status {
        stub_status on;
        access_log off;
    }
}
